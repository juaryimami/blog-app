 <% @users.each do |user| %>
 <div class="users-wrapper flex">
    <div class="img-cont">
     <img class="img-cont", src ="<%= user.photo %>", size="70x70", alt="userphoto">
    </div>
    <div class="user-details rounded-2xl">
      <h2><%= link_to user.name, user, class: "link-button"  %></h2>
      <p>Number of posts: <%= user.post_counter %></p>
    </div>
 </div>
 <%end%>